@{
    Layout = "~/Views/Admin/_Admin_Layout.cshtml";
}
@using AlbumForU.Models
@model List<AppUser>
<a class="btn" asp-action="AdminIndex">Back</a>
<div class="alert-danger">
    @Html.ValidationSummary()
</div>
<div class="users_list">
    <ul>
        @foreach (var user in Model)
        {
            <li>
                <a class="list-group-item" href="~/Admin/AppointCertainUser/@user.Id">@user.Firstname @user.Lastname</a>
            </li>
        }
    </ul>
</div>